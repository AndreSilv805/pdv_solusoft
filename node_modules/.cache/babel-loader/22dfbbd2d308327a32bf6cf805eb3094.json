A,OAAA,CAFA;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,OAAA,CAAA,GAAA,CAAA,cAAA,EAAA,QAAA;AACA,gBAAA,KAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA;;AAJA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KANA;AAOA,IAAA,YAPA,0BAOA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,iEAAA,MAAA,CAAA,EAAA,EAFA;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,OAAA,CAAA,GAAA,CAAA,cAAA,EAAA,QAAA;AACA,gBAAA,MAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA;;AAJA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA;AAZA;AAhCA,CAAA","sourcesContent":["<template>\r\n    <div class=\"container py-4\"> \r\n     <div class=\"row\">\r\n            <div class=\"col-sm-10\">\r\n                <h1 class=\"font-weight-light\">Editar de Clientes</h1>\r\n            </div>\r\n            <div class=\"col-sm-2\">\r\n                <router-link \r\n                    :to=\"`/clientes`\"\r\n                    class=\"btn btn-info btn-sm float-right\">\r\n                    <span>Clientes</span>\r\n                </router-link>\r\n            </div>\r\n        </div>\r\n\r\n        <hr>\r\n    <b-form @submit=\"editarCliente\"><!--formulário criar produto-->\r\n\r\n      <b-form-group id=\"input-group-1\" label=\"Cliente\" label-for=\"input-cliente\">\r\n        <b-form-input\r\n          id=\"input-cliente\"\r\n          v-model=\"cliente.nome\"\r\n          type=\"text\"\r\n          required\r\n          placeholder=\"Entre com nome do cliente\"\r\n        ></b-form-input>\r\n      </b-form-group>\r\n\r\n       <b-form-group id=\"input-group-3\" label=\"e-mail\" label-for=\"input-email\">\r\n        <b-form-input\r\n          id=\"input-email\"\r\n          v-model=\"cliente.email\"\r\n          placeholder=\"Digite o e-mail \"\r\n          required\r\n        ></b-form-input>\r\n      </b-form-group>\r\n\r\n      <b-form-group id=\"input-group-2\" label=\"Código\" label-for=\"input-codigo\">\r\n        <b-form-input\r\n          id=\"input-codigo\"\r\n          v-model=\"cliente.cod_cliente\"\r\n          required\r\n          placeholder=\"Digite o código \"\r\n        ></b-form-input>\r\n      </b-form-group>\r\n\r\n      <b-form-group id=\"input-group-3\" label=\"cpf\" label-for=\"input-cpf\">  \r\n        <the-mask class=\"form-control\" required id=\"input-cpf\" placeholder=\"Digito o CPF\" :mask=\"['###.###.###-##']\" v-model=\"cliente.cpf\" />\r\n     </b-form-group>\r\n\r\n      <b-form-group label=\"sexo\">\r\n      <b-form-radio-group\r\n        v-model=\"cliente.sexo\"\r\n        :options=\"options\"\r\n        name=\"radio-inline\"\r\n      ></b-form-radio-group>\r\n     </b-form-group>\r\n\r\n     \r\n\r\n      <b-button type=\"submit\" variant=\"primary\">Salvar</b-button>\r\n\r\n       </b-form>\r\n\r\n    {{cliente}}\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from 'axios'\r\nimport {TheMask} from 'vue-the-mask'\r\n\r\nexport default {\r\n    components: {TheMask},\r\n    props: {\r\n        id: {\r\n            type: Number,\r\n            required: true,    \r\n        }\r\n    },\r\n    data() {\r\n         return {\r\n            mensagemErro: undefined,\r\n             cliente: {\r\n                cod_cliente:'',\r\n                nome: '',\r\n                cpf: '',\r\n                sexo:'',\r\n                email:'',\r\n            },\r\n                options: [\r\n                  { text: 'feminino', value: 'f' },\r\n                  { text: 'masculino', value: 'm' },\r\n                ]\r\n    \r\n         }\r\n    },\r\n    beforeRouteEnter(to, from, next) {\r\n        next(vm => { \r\n            vm.pegarCliente();\r\n        })\r\n    },\r\n   \r\n    \r\n    methods:{\r\n        async editarCliente(){\r\n\r\n            const response = await axios.put(`http://127.0.0.1/pdvsolusoft/blog/public/api/clientes/${this.id}`,this.cliente);\r\n            console.log('GET /cliente', response)\r\n            this.cliente = response.data;\r\n        },\r\n        async pegarCliente(){\r\n\r\n            const response = await axios.get(`http://127.0.0.1/pdvsolusoft/blog/public/api/clientes/${this.id}`);\r\n            console.log('GET /cliente', response)\r\n            this.cliente = response.data;\r\n        },\r\n    }\r\n\r\n}\r\n</script>\r\n"],"sourceRoot":"src/views/clientes"}]}