ail\">\r\n            </div>\r\n\r\n            <button \r\n                type=\"button\" \r\n                class=\"btn btn-secondary mt-4 mb-4 mr-2\" \r\n                @click=\"$router.back()\">\r\n                    Voltar\r\n            </button>\r\n            <button \r\n                type=\"submit\" \r\n                class=\"btn btn-success\">\r\n                    Salvar\r\n            </button>\r\n        </form>\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport EventBus from './../../event-bus'\r\n\r\n\r\nexport default {\r\n    props: ['id'],\r\n    data() {\r\n        return {\r\n            contato: undefined,\r\n            estaCancelando: true,\r\n            mensagemErro: undefined,\r\n            produto: {\r\n                cod_produto:'',\r\n                nome: '',\r\n                valor:null, \r\n                corv: ['apple', 'orange', 'grape'],\r\n                tam: ['apple', 'orange', 'grape'], \r\n            }\r\n        }\r\n    },\r\n\r\n    beforeRouteEnter(to, from, next) {\r\n        next(vm => {\r\n            vm.contato = EventBus.buscarContato(+to.params.id);\r\n            \r\n        })\r\n    },\r\n\r\n    beforeRouteLeave(to, from, next) {\r\n        console.log('beforeRouteLeave')\r\n        this.estaCancelando\r\n            ? next(window.confirm('Deseja realmente sair?'))\r\n            : next()\r\n    },\r\n\r\n    methods: {\r\n        salvar() {\r\n            EventBus.editarContato(this.contato)\r\n            this.estaCancelando = false\r\n            this.$router.push('/contatos')\r\n        },\r\n       \r\n    }\r\n}\r\n</script>\r\n\r\n"]}]}