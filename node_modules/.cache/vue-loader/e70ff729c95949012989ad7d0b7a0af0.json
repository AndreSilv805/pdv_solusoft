n\r\n    </div>-->\r\n     <div> \r\n     <div class=\"row\">\r\n            <div class=\"col-sm-10\">\r\n                <h1 class=\"font-weight-light\">Detalhes e Edição do Produto</h1>\r\n            </div>\r\n            <div class=\"col-sm-2\">\r\n                <router-link \r\n                    :to=\"`/produtos`\"\r\n                    class=\"btn btn-info btn-sm float-right\">\r\n                    <span>Lista de Produtos</span>\r\n                </router-link>\r\n            </div>\r\n        </div>\r\n\r\n        <hr>\r\n\r\n    <b-form class=\"mb-5\" @submit=\"criarProduto\"><!--formulário criar produto-->\r\n      <b-form-group\r\n        id=\"input-group-1\"\r\n        label=\"Produto\"\r\n        label-for=\"input-produto\"\r\n      >\r\n        <b-form-input\r\n          id=\"input\"\r\n          v-model=\"produto.nome\"\r\n          type=\"text\"\r\n          required\r\n          placeholder=\"Entre com nome do produto\"\r\n        ></b-form-input>\r\n      </b-form-group>\r\n\r\n      <b-form-group id=\"input-group-2\" label=\"Código\" label-for=\"input-codigo\">\r\n        <b-form-input\r\n          id=\"input-codigo\"\r\n          v-model=\"produto.cod_produto\"\r\n          required\r\n          placeholder=\"Digite o código \"\r\n        ></b-form-input>\r\n      </b-form-group>\r\n\r\n      <b-form-group id=\"input-group-3\" label=\"Valor\" label-for=\"input-valor\">\r\n        <b-form-input\r\n          id=\"input-valor\"\r\n          v-model=\"valorbr\"\r\n          placeholder=\"Digite o valor \"\r\n          required\r\n        ></b-form-input>\r\n      </b-form-group>\r\n      \r\n      <h6 class=\"mb-2\">OBJ PRODUTO PARA ADICIONAR: {{produto}}</h6><br>\r\n      \r\n  \r\n        <label class=\"my-2\" for=\"tags-pills\">Informe as cores  do Produto</label>\r\n        <b-form-tags\r\n          input-id=\"tags-pills\"\r\n          v-model=\"produto.cores\"\r\n          tag-variant=\"info\"\r\n          tag-pills\r\n          separator=\" \"\r\n          placeholder=\"Informe as cores\"\r\n          :disabled=false\r\n        ></b-form-tags>\r\n       \r\n\r\n        <label class=\"my-2\" for=\"tags-pills\">Informe os tamanhos</label>\r\n        <b-form-tags\r\n          input-id=\"tags-pills\"\r\n          v-model=\"produto.tamanhos\"\r\n          tag-variant=\"info\"\r\n          tag-pills\r\n          separator=\" \"\r\n          placeholder=\"Informe os Tamanhos\"\r\n        ></b-form-tags>\r\n       \r\n\r\n\r\n      <b-button class=\"mt-4\" type=\"submit\" variant=\"primary\">Salvar</b-button>\r\n\r\n       </b-form><!--fim criar produto-->\r\n\r\n    </div>\r\n\r\n     <h3 class=\"font-weight-light mb-4\">Variação dos produtos</h3>\r\n     <hr>\r\n\r\n    <!-- <div  v-for=\"cor in produto.cores\" :key=\"cor\">\r\n        <div v-for=\"tamanho in produto.tamanhos\" :key=\"tamanho\">\r\n\r\n            <h6 class=\"text-info\">{{cor}} / {{tamanho}}</h6> <hr class=\"border-info\">\r\n            <div class=\"row\">\r\n            <b-form-group class=\"col-lg-6\" id=\"input-group-2\" label=\"Código\" label-for=\"input-codigo\">\r\n                <b-form-input\r\n                id=\"input-codigo\"\r\n                v-model=\"produto.cod_produto\"\r\n                required\r\n                placeholder=\"Digite o código \"\r\n                ></b-form-input>\r\n            </b-form-group>\r\n            \r\n             <b-form-group class=\"col-lg-6\" id=\"input-group-3\" label=\"Valor\" label-for=\"input-valor\">\r\n                <b-form-input\r\n                id=\"input-valor\"\r\n                v-model=\"produto.valor\"\r\n                placeholder=\"Digite o valor \"\r\n                required\r\n                ></b-form-input>\r\n            </b-form-group>\r\n            </div>-->\r\n\r\n        <!--</div>\r\n    </div>-->\r\n    <div  v-for=\"variacao in produto.variations\" :key=\"variacao\">\r\n        <h6 class=\"text-info\">{{variacao.nome}}</h6> <hr class=\"border-info\">\r\n\r\n        \r\n    </div>\r\n    </div>\r\n\r\n</template>\r\n\r\n<script>\r\n\r\nimport EventBus from './../../event-bus'\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    props: {\r\n        id: {\r\n            type: Number,\r\n            required: true,    \r\n        }\r\n    },\r\n    data() {\r\n        return {\r\n            contato: undefined,\r\n            mensagemErro: undefined,\r\n            produto: {\r\n                cod_produto:'',\r\n                nome:'',\r\n                valor:null, \r\n                cores: [],\r\n                tamanhos: [], \r\n                variations: [],\r\n            }\r\n        }\r\n    },\r\n    \r\n    /*created() {\r\n        this.contato = EventBus.buscarContato(this.id)\r\n    },*/\r\n    beforeRouteEnter(to, from, next) {\r\n        next(vm => {\r\n            // vm.contato = EventBus.buscarContato(vm.id)\r\n            vm.pegarProduto();\r\n            vm.contato = EventBus.buscarContato(+to.params.id)\r\n        })\r\n    },\r\n    beforeRouteUpdate(to, from, next) {\r\n        this.contato = EventBus.buscarContato(+to.params.id)\r\n        next()\r\n    },\r\n    watch: {\r\n    produto: function (value) {\r\n      return value.valor.toLocaleString('pt-br',{style:'currency',currency:'BRL'})\r\n       },\r\n    },\r\n    computed:{\r\n   valorbr: function () {\r\n        return this.produto.valor.toLocaleString('pt-br',{style:'currency',currency:'BRL'})\r\n        }\r\n    },\r\n    \r\n    methods:{\r\n         criarProduto() {\r\n            \r\n            axios.put(`http://127.0.0.1/pdvsolusoft/blog/public/api/produtos/${this.id}`,this.produto)\r\n                .then((response) => {\r\n                    console.log('Produto criado com sucesso', response)\r\n                })\r\n\r\n                this.$router.push('/produtos')\r\n            },\r\n        async editarProduto(){\r\n\r\n            const response = await axios.put(`http://127.0.0.1/pdvsolusoft/blog/public/api/produtos/${this.id}`,this.produto);\r\n            console.log('GET /produtos', response)\r\n            this.produto = response.data;\r\n        },\r\n        async pegarProduto(){\r\n\r\n            const response = await axios.get(`http://127.0.0.1/pdvsolusoft/blog/public/api/produtos/${this.id}`);\r\n            console.log('GET /produtos', response)\r\n            this.produto = response.data;\r\n        },\r\n    }\r\n    \r\n}\r\n\r\n</script>\r\n\r\n"]}]}