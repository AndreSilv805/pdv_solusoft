r\n          v-model=\"produto.nome\"\r\n          type=\"text\"\r\n          required\r\n          placeholder=\"Entre com nome do produto\"\r\n        ></b-form-input>\r\n      </b-form-group>\r\n\r\n      <b-form-group id=\"input-group-2\" label=\"Código\" label-for=\"input-codigo\">\r\n        <b-form-input\r\n          id=\"input-codigo\"\r\n          v-model=\"produto.cod_produto\"\r\n          required\r\n          placeholder=\"Digite o código \"\r\n        ></b-form-input>\r\n      </b-form-group>\r\n\r\n      <b-form-group id=\"input-group-3\" label=\"Valor\" label-for=\"input-valor\">\r\n        <b-form-input\r\n          id=\"input-valor\"\r\n          v-model=\"produto.valor\"\r\n          placeholder=\"Digite o valor \"\r\n          required\r\n        ></b-form-input>\r\n      </b-form-group>\r\n\r\n      \r\n  \r\n        <label class=\"my-2\" for=\"tags-pills\">Informe as cores  do Produto</label>\r\n        <b-form-tags\r\n          input-id=\"tags-pills\"\r\n          v-model=\"produto.cores\"\r\n          tag-variant=\"primary\"\r\n          tag-pills\r\n          separator=\" \"\r\n          placeholder=\"Informe as cores\"\r\n        \r\n        ></b-form-tags>\r\n       \r\n\r\n        <label class=\"my-2\" for=\"tags-pills\">Informe os tamanhos</label>\r\n        <b-form-tags\r\n          input-id=\"tags-pills\"\r\n          v-model=\"produto.tamanhos\"\r\n          tag-variant=\"primary\"\r\n          tag-pills\r\n          separator=\" \"\r\n          placeholder=\"Informe os Tamanhos\"\r\n        ></b-form-tags>\r\n       \r\n\r\n\r\n      <b-button class=\"mt-4\" type=\"submit\" variant=\"primary\">Salvar</b-button>\r\n\r\n       </b-form><!--fim criar produto-->\r\n       \r\n       \r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    data() {\r\n         return {\r\n        \r\n            mensagemErro: undefined,\r\n             produto: {\r\n                cod_produto:'',\r\n                nome: '',\r\n                valor:null, \r\n                cores: ['apple', 'orange', 'grape'],\r\n                tamanhos: ['apple', 'orange', 'grape'], \r\n            },\r\n            \r\n    \r\n         }\r\n    },\r\n    methods: {\r\n    criarProduto() {\r\n            \r\n            axios.post(`http://127.0.0.1/pdvsolusoft/blog/public/api/produtos`,this.produto)\r\n                .then((response) => {\r\n                    console.log('Produto criado com sucesso', response)\r\n                })\r\n\r\n                this.$router.push('/produtos')\r\n            },\r\n            \r\n    },\r\n\r\n}\r\n</script>\r\n\r\n"]}]}